<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssafy.zipsee.user.model.mapper.InterestMapper">
	<select id="getInterest" parameterType="String" resultType="com.ssafy.zipsee.user.model.InterestDto">
		select *
		from interest
		where interest_id = #{interestId}
	</select>
</mapper>